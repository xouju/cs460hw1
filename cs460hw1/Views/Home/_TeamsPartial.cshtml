@model TeamCreatorViewModel

<h3>Generated Teams</h3>
@if (Model.Teams != null && Model.Teams.Any())
{
    <div class="teams-container">
        @for (int i = 0; i < Model.Teams.Count; i++)
        {
            <div class="team">
                <h4>Team @i</h4>
                <ul>
                    @foreach (var member in Model.Teams[i])
                    {
                        <li>@member</li>
                    }
                </ul>
            </div>
        }
    </div>
}
else
{
    <p>No teams generated.</p>
}
